<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="100.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sciencerec.ArticleListsController">
   <children>
      <HBox prefHeight="100.0" prefWidth="100.0">
         <children>
            <Button fx:id="addArticle" minWidth="100" mnemonicParsing="false" onAction="#onAddArticleClicked" text="AddArticle">
               <HBox.margin>
                  <Insets top="30.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="addUser" minWidth="100" mnemonicParsing="false" onAction="#onAddUserClicked" text="AddUser">
               <HBox.margin>
                  <Insets left="10" top="30.0" />
               </HBox.margin>
            </Button>
         </children></HBox>
   </children>
</AnchorPane>
